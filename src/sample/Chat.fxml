<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<FlowPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <FlowPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="360.0" prefWidth="600.0">
         <children>
            <ListView fx:id="ChatLV" prefHeight="360.0" prefWidth="440.0">
               <FlowPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </FlowPane.margin>
            </ListView>
            <ListView fx:id="selectUserLV" prefHeight="341.0" prefWidth="130.0">
               <FlowPane.margin>
                  <Insets bottom="10.0" top="10.0" />
               </FlowPane.margin>
            </ListView>
         </children>
      </FlowPane>
      <FlowPane prefHeight="40.0" prefWidth="600.0">
         <children>
            <TextField fx:id="textTF" prefHeight="25.0" prefWidth="440.0">
               <FlowPane.margin>
                  <Insets left="10.0" />
               </FlowPane.margin>
            </TextField>
            <Button fx:id="sendB" mnemonicParsing="false" onAction="#sendMessageOnClick" prefHeight="25.0" prefWidth="130.0" text="Send">
               <FlowPane.margin>
                  <Insets left="10.0" right="10.0" />
               </FlowPane.margin>
            </Button>
         </children>
      </FlowPane>
   </children>
</FlowPane>
